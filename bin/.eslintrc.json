        "@typescript-eslint/no-implicit-any-catch": "off",
        "@typescript-eslint/no-magic-numbers": "off",
        "@typescript-eslint/prefer-optional-chain": "off",
        "@typescript-eslint/tslint/config": ["error", {
            "lintFile": "./bin/tslint.json"
        }]
